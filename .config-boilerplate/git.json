{
	"user": {
		"name": "your-name-here",
		"email": "your-email-here"
	},
	"services": {
		"github": {
		    "token": "your-github-token-here"
		},
		"gitlab": {
		    "https": true,
		    "domain": "gitlab.com",
		    "port": 22,
		    "api-version": "v3",
		    "token": "your-gitlab-token-here",
		    "upstream": true,
		    "config-repo": "workspace-config"
		}
	},
	"global-ignore": [
		".DS_Store",
		".AppleDouble",
		".LSOverride",
		"Icon",
		"._*",
		".Spotlight-V100",
		".Trashes",
		".AppleDB",
		".AppleDesktop",
		".apdisk",
		"Thumbs.db",
		"ehthumbs.db",
		"Desktop.ini",
		"$RECYCLE.BIN/",
		"*.cab",
		"*.msi",
		"*.msm",
		"*.msp"
	]
}